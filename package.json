{
  "name": "@tmmc/animate-scss",
  "version": "1.0.5",
  "description": "Sass version of cross-browser CSS animations library by Daniel Eden.",
  "keywords": [
    "css animations",
    "css",
    "animations"
  ],
  "main": "src/sass/animate.scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/TMMC/animate-scss.git"
  },
  "author": {
    "name": "TMMC",
    "url": "https://tmmc.pl/"
  },
  "homepage": "https://daneden.github.io/animate.css/",
  "scripts": {
    "build": "gulp build",
    "start": "gulp buildWatch",
    "watch": "gulp watcher",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "MIT",
  "devDependencies": {
    "ansi-colors": "^3.2.1",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-header": "^2.0.5",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "node-sass": "^4.10.0"
  },
  "browserslist": [
    ">= 3%",
    "last 3 versions",
    "not dead",
    "Android >= 4.4",
    "Chrome >= 40",
    "Firefox >= 30",
    "Opera >= 20",
    "IE >= 10",
    "Edge >= 10"
  ],
  "config": {
    "src": "src/sass",
    "dist": "dist",
    "origin": {
      "name": "animate.css",
      "version": "3.7.0",
      "homepage": "https://daneden.github.io/animate.css/",
      "author": {
        "name": "Daniel Eden",
        "url": "https://daneden.me/"
      }
    },
    "autoprefixerOptions": {
      "cascade": true,
      "add": true,
      "remove": true,
      "flexbox": true,
      "grid": false
    },
    "sassOptions": {
      "dev": {
        "errLogToConsole": false,
        "outputStyle": "expanded"
      },
      "prod": {
        "errLogToConsole": false,
        "outputStyle": "compressed"
      }
    }
  }
}
